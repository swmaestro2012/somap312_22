<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="horizontal" >

    <TextView
        android:id="@+id/adapterview_select_building_floor_list_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView" />
    <View  android:layout_width="0dip"
        android:layout_height="wrap_content"
        android:layout_weight="1"/>

    <Button
        android:id="@+id/adapterview_select_building_floor_list_delete"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="삭제" />

    <Button
        android:id="@+id/adapterview_select_building_floor_list_select"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="선택" />

    <Button
        android:id="@+id/adapterview_select_building_floor_list_collect"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="수집" />

</LinearLayout>